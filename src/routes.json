{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "fhir2": "^1.2.0",
    "webservices.rest": "^2.2.0"
  },
  "pages": [
    {
      "component": "root",
      "routeRegex": "forms",
      "online": true,
      "offline": true
    }
  ],
  "extensions": [
    {
      "name": "forms-app-link",
      "slot": "app-menu-slot",
      "component": "formsAppMenuLink",
      "online": true,
      "offline": true
    }
  ]
}